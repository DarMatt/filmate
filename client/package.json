  "private": true,
  "homepage": "https://darmatt.github.io/film-app/",
  "description": "movie-app",
  "main": "index.tsx",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack --mode development",
    "build": "cross-env NODE_ENV=production webpack serve --mode production",
    "watch": "webpack --mode development --watch",
    "start": "cross-env NODE_ENV=development webpack serve --mode development --open",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "lint": "eslint --fix src/ --ext .ts,.tsx,.js,.jsx",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.+(js|jsx|ts|tsx)": [
      "eslint --fix",
      "git add"
    ],
    "*.+(json|css|md|ts|tsx|js|jsx)": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DarMatt/filmate.git"
  },
  "keywords": [
    "js",
    "javascript",
    "webpack",
    "react",
    "hooks"
  ],
